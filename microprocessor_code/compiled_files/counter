#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x635cccdce490 .scope module, "counter_4bit_tb" "counter_4bit_tb" 2 60;
 .timescale 0 0;
v0x635ccce1b340_0 .var "clk", 0 0;
v0x635ccce1b410_0 .net "count", 15 0, L_0x635ccce1b5e0;  1 drivers
v0x635ccce1b4e0_0 .var "reset", 0 0;
S_0x635cccdce620 .scope module, "uut" "counter_4bit" 2 66, 3 1 0, S_0x635cccdce490;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 16 "count";
L_0x635ccce1b5e0 .functor BUFZ 16, v0x635ccce1b140_0, C4<0000000000000000>, C4<0000000000000000>, C4<0000000000000000>;
v0x635cccdce850_0 .net "clk", 0 0, v0x635ccce1b340_0;  1 drivers
v0x635ccce1b060_0 .net "count", 15 0, L_0x635ccce1b5e0;  alias, 1 drivers
v0x635ccce1b140_0 .var "count_reg", 15 0;
v0x635ccce1b200_0 .net "reset", 0 0, v0x635ccce1b4e0_0;  1 drivers
E_0x635ccce09be0 .event posedge, v0x635ccce1b200_0, v0x635cccdce850_0;
    .scope S_0x635cccdce620;
T_0 ;
    %wait E_0x635ccce09be0;
    %load/vec4 v0x635ccce1b200_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x635ccce1b140_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x635ccce1b140_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x635ccce1b140_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x635cccdce490;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x635ccce1b340_0;
    %inv;
    %store/vec4 v0x635ccce1b340_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x635cccdce490;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x635ccce1b340_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x635ccce1b4e0_0, 0, 1;
    %vpi_call 2 78 "$monitor", "Time: %0t, count: %d, reset: %b", $time, v0x635ccce1b410_0, v0x635ccce1b4e0_0 {0 0 0};
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x635ccce1b4e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x635ccce1b4e0_0, 0, 1;
    %delay 50, 0;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x635ccce1b4e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x635ccce1b4e0_0, 0, 1;
    %delay 50, 0;
    %delay 10, 0;
    %vpi_call 2 90 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "main.v";
    "Counter.v";
